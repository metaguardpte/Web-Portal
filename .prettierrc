{
  "tabWidth": 4,
  "printWidth": 100,
  "parser": "typescript",
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "lf",
  "proseWrap": "never",
  "overrides": [
    { "files": ".prettierrc", "options": { "parser": "json" } }, 
    { "files": "*.less", "options": { "parser": "less" } },
    { "files": "*.json", "options": { "parser": "json" } }
  ],
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-packagejson"]
}
